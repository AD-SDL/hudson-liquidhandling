{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Schema for SoloSoft MoveArm Step",
    "type": "object",
    "properties": {
        "metadata": {
            "$ref": "../../metadata.json"
        },
        "step_type": {
            "const": "MoveArm"
        },
        "desination": {
            "type": "string"
        },
        "xyz_speed": {
            "type": "number",
            "minimum": 0
        },
        "move_z_at_start": {
            "type": "boolean"
        }
    },
    "required": [
        "step_type"
    ],
    "additionalProperties": false
}